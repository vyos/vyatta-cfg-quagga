multi:
type: ipv6net
help: RIPng static route

syntax:expression: exec "ipaddrcheck --verbose --is-ipv4-net $VAR(@)"

create: vtysh -c "configure terminal" \
          -c "router ripng"                                       \
          -c "route $VAR(@)"

delete: vtysh -c "configure terminal" \
          -c "router ripng"                                       \
          -c "no route $VAR(@)"

